#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int a,b,c;
	    scanf("%d%d",&a,&b);
	    c=a+b;
	    if(c=0){
	        printf("%d",0);
	    }
	    else{
	        if(a>=b){
	            printf("%d",7-a);
	        }
	        else{
	            printf("%d",7-b);
	        }
	}
	printf("\n");
	}
	return 0;
}

