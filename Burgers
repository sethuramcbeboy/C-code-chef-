#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int a,b;
	    scanf("%d",&a);
	    scanf("%d",&b);
	    if(a==b){
	        printf("%d",a);
	    }
	    else if(a>b){
	        printf("%d",b);
	    }
	    else{
	        printf("%d",a);
	    }
	    printf("\n");
	}
	return 0;
}
