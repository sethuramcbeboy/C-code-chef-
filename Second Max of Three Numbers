#include <stdio.h>

int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int a,b,c;
	    scanf("%d",&a);
	    scanf("%d",&b);
	    scanf("%d",&c);
	    if((a>b && a<c) || (a<b && a>c)){
	        printf("%d",a);
	    }
	    else{
	        if((b>c && b<a) || (b<c && b>a)){
	            printf("%d",b);
	        }
	        else{
	            printf("%d",c);
	        }
	    }
	    printf("\n");
	}
	return 0;
}

